{
  "name": "alfabank",
  "version": "1.0.0-alpha.1",
  "description": "Pay API for alfa-biz.by (alfabank belarus)",
  "main": "index.js",
  "scripts": {
    "dev": "vite-node",
    "start": "esno -r dotenv/config ./play/index.ts",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf dist",
    "publish": "npm run clean && npm run build && npm version prerelease --preid=alpha && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whalest/alfabankby.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "alfabank",
    "alfa-biz",
    "belarus"
  ],
  "author": "reslear",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/whalest/alfabankby/issues"
  },
  "homepage": "https://github.com/whalest/alfabankby#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "qs": "^6.9.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "@types/qs": "^6.9.5",
    "axios-nice-log": "^0.1.7",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "tsup": "^4.3.1",
    "typescript": "^4.2.2",
    "vite": "^2.0.3",
    "vite-plugin-node": "^0.0.7"
  }
}
